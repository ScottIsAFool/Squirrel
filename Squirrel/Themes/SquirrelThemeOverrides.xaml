<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Resource dictionary entries should be defined here. -->

    <Color x:Key="PhoneBackgroundColor">#086FA1</Color>
    <SolidColorBrush x:Key="PhoneBackgroundBrush"
                     Color="{StaticResource PhoneBackgroundColor}" />

    <Color x:Key="PhoneAccentColor">#E3C800</Color>
    <SolidColorBrush x:Key="PhoneAccentBrush"
                     Color="{StaticResource PhoneAccentColor}" />
</ResourceDictionary>